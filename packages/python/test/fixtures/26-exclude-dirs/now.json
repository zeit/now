{
  "version": 2,
  "builds": [
    { "src": "api/all.py", "use": "@vercel/python" },
    {
      "src": "api/none.py",
      "use": "@vercel/python",
      "config": { "excludeFiles": "ignoreme/**" }
    },
    {
      "src": "api/one.py",
      "use": "@vercel/python",
      "config": {
        "excludeFiles": "ignoreme/**",
        "includeFiles": "ignoreme/second/**"
      }
    }
  ],
  "probes": [
    {
      "path": "/api/all.py",
      "mustContain": "True:True:RANDOMNESS_PLACEHOLDER"
    },
    {
      "path": "/api/none.py",
      "mustContain": "False:False:RANDOMNESS_PLACEHOLDER"
    },
    {
      "path": "/api/one.py",
      "mustContain": "False:True:RANDOMNESS_PLACEHOLDER"
    }
  ]
}
